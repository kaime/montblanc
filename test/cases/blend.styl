// @describe `blend()`


// @it `normal`
body
  color: blend('normal', grey(25%), grey(25%))
  color: blend('normal', yellowgreen, grey(25%))
  color: blend('normal', grey(25%), yellowgreen)
  color: blend('normal', yellowgreen, yellowgreen)

// @expect
body {
  color: #bfbfbf;
  color: #9acd32;
  color: #bfbfbf;
  color: #9acd32;
}

// @it `multiply`
body
  color: blend('multiply', grey(25%), grey(25%))
  color: blend('multiply', yellowgreen, grey(25%))
  color: blend('multiply', grey(25%), yellowgreen)
  color: blend('multiply', yellowgreen, yellowgreen)

// @expect
body {
  color: #8f8f8f;
  color: #749a26;
  color: #749a25;
  color: #5da50a;
}