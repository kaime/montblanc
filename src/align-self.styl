align-self(mode, args...)
  // Old
  if has-support('ie < 11', 'ie-mobile < 11')
    if mode is flex-start
      old-mode = start
    else if mode is flex-end
      old-mode = end
    else
      old-mode = mode

    vendorize('flex-item-align', old-mode args, (ms), false)

  // New syntax
  vendors = ()

  if has-support('firefox < 22')
    push(vendors, moz)

  if has-support(
        'chrome < 29',
        'opera < 17',
        'ios-safari',
        'blackberry-browser'
      )
    push(vendors, webkit)

  vendorize('align-self', arguments, vendors)
