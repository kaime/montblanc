initial-values = json('data/initial-values.json', { hash: true })

initial()
  property = cast(shift(arguments), 'string')

  if length(arguments) > 0
    value = unquote('')
    for arg, i  in arguments
      if i > 0
        value = '%s,' % value
      value = '%s %s' % (value arg)
    initial-values[property] = value
  else
    if property in initial-values
      return initial-values[property]
    else
      return null

