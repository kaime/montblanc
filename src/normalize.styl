normalize()
  *,
  :before,
  :after
    if initial('box-sizing') isnt content-box
      box-sizing: initial('box-sizing')

    if initial('transition-duration') isnt 0
      transition-duration: initial('transition-duration')

    if initial('transition-delay') isnt 0
      transition-delay: initial('transition-delay')

    if initial('transition-timing-function') isnt ease
      transition-timing-function: initial('transition-timing-function')

    if initial('transform-origin) isnt (50% 50%')
      transform-origin: initial('transform-origin')

    if initial('backface-visibility') isnt visible
      backface-visibility: initial('backface-visibility')

    if initial('animation-duration') isnt 0
      animation-duration: initial('animation-duration')

    if initial('animation-delay') isnt 0
      animation-delay: initial('animation-delay')

    if initial('animation-timing-function') isnt ease
      animation-timing-function: initial('animation-timing-function')

  html
    // Set default font family
    font-family: initial('font-family')

    if initial('font-size') isnt medium
      font-size: initial('font-size')

    if initial('color') isnt #000
      color: initial('color')

    if initial('line-height') isnt normal
      line-height: initial('line-height')

    if initial('direction') isnt ltr
      direction: initial('direction')

    if initial('tab-size') isnt 8
      tab-size: initial('tab-size')

    if has-support('ie')
      // Correct text resizing oddly in IE when body `font-size` is set
      // using `em` units.
      -ms-text-size-adjust: 100%

    if has-support('ios-safari')
      // Prevent iOS text size adjust after orientation change, without
      // disabling user zoom.
      -webkit-text-size-adjust: 100%

  body
    // Remove default margin.
    margin: 0

  if has-support('ie < 10')
    // Correct `block` display not defined for any HTML5 element in IE 6/7/8/9
    article,
    aside,
    figcaption,
    figure,
    footer,
    header,
    hgroup,
    nav,
    section
      display: block

  if has-support('ie', 'firefox')
    // Correct `block` display not defined for `details` or `summary` in IE
    // 10/11 and Firefox.
    details,
    summary
      display: block

  if has-support('ie')
    // Correct `block` display not defined for `main` in IE 11.
    main
      display: block

  audio,
  canvas,
  progress,
  video
    // Correct `inline-block` display not defined in IE 6/7/8/9 and Firefox 3.
    if has-support('ie < 10', 'firefox < 4')
      display: inline-block

    // Normalize vertical alignment of `progress` in Chrome, Firefox, and Opera.
    if has-support('firefox', 'opera', 'chrome')
      vertical-align: baseline

  audio:not([controls])
    // Prevent modern browsers from displaying `audio` without controls.
    display: none
    if has-support('ios-safari <= 5')
      // Remove excess height in iOS 5 devices.
      height: 0

  // Address `[hidden]` styling not present in IE 7/8/9/10, Firefox 3 and
  // Safari 4. Known error: no IE 6 support.
  if has-support('ie < 11')
    [hidden]
      display: none

  // Hide the `template` element in IE 8/9/11, Safari, and Firefox < 22.
  if has-support('ie', 'safari', 'firefox < 22')
    template
      display: none

  a
    // Remove the gray background color from active links in IE 10.
    if has-support('ie < 11')
      background: transparent

    // Improve readability when focused and also mouse hovered in all browsers.
    &:active,
    &:hover
      outline: 0

  // Address styling not present in IE 7/8/9/10/11 Safari, and Chrome.
  if has-support('ie', 'safari', 'chrome')
    abbr[title]
      border-bottom: 1px dotted

  if has-support('firefox', 'safari', 'chrome')
    // Address style set to `bolder` in Firefox, Safari, and Chrome.
    b,
    strong
      font-weight: bold

  //Address styling not present in Safari and Chrome.
  if has-support('safari', 'chrome')
    dfn
      font-style: italic

  if has-support('firefox', 'safari', 'chrome')
    // Address variable `h1` font-size and margin within `section` and `article`
    // contexts in Firefox, Safari and Chrome.
    h1
      font-size: 2em
      margin: 0.67em 0

  // Address styling not present in IE 6/7/8/9.
  if has-support('ie < 10')
    mark
      background: yellow
      color: black

  if has-support('ie < 8')
    // Address margins set differently in IE 6/7.
    p,
    pre
      margin: 1em 0

  if  has-support('ie < 8')
    // Address CSS quotes not supported in IE 6/7.
    q
      quotes: none

  if has-support('safari < 5')
    // Address `quotes` property not supported in Safari 4.
    q:before,
    q:after
      content: ''
      content: none

  // Address inconsistent and variable font size in all browsers.
  small
    font-size: 80%

  // Prevent `sub` and `sup` affecting `line-height` in all browsers.
  sub,
  sup
    font-size: 75%
    line-height: 0
    position: relative
    vertical-align: baseline

  sup
    top: -0.5em

  sub
    bottom: -0.25em

  if has-support('ie < 8')
    // Address margins and paddings set differently in IE 6/7.
    dl,
    menu,
    ol,
    ul
      margin: 1em 0

    dd
      margin: 0 0 0 40px

    menu,
    ol,
    ul
      padding: 0 0 0 40px

    // Correct list images handled incorrectly in IE 7.
    nav
      ul,
      ol
        list-style: none
        list-style-image: none

  // Remove border when inside `a` element in IE 6/7/8/9 and Firefox 3.
  if has-support('ie < 10', 'firefox < 4')
    img
      border: 0
      if has-support('ie < 8')
        // Improve image quality when scaled in IE 7.
        -ms-interpolation-mode: bicubic

  // Correct overflow displayed oddly in IE 9/10/11.
  if has-support('ie')
    svg:not(:root)
      overflow: hidden

  // Address margin not present in IE 6/7/8/9, Safari and Opera 11.
  if has-support('ie < 10', 'safari', 'opera < 12')
    figure
      margin: 1em 40px

  if has-support('firefox')
    // Address differences between Firefox and other browsers.
    // Known issue: no IE 6/7 normalization.
    hr
      if initial('box-sizing') is content-box
        box-sizing: content-box
      height: 0

  // Contain overflow in all browsers.
  pre
    overflow: auto

  // Address odd `em`-unit font size rendering in all browsers.
  code,
  kbd,
  pre,
  samp
    font-family: monospace, monospace
    font-size: 1em

  if has-support('ie < 8')
    // Correct margin displayed oddly in IE 6/7.
    form
      margin: 0

  // Known limitation: by default, Chrome and Safari on OS X allow very limited
  // styling of `select`, unless a `border` property is set.

  button,
  input,
  optgroup,
  select,
  textarea
    // Correct color not being inherited. Known issue: affects color of disabled
    // elements.
    color: inherit

    // Correct font properties not being inherited.
    font: inherit

    // Address margins set differently in IE 6/7, Firefox 3+, Safari 5, and
    // Chrome.
    if has-support('ie < 8', 'firefox < 4', 'safari', 'chrome')
      margin: 0
      // Improve appareance and consistency
      vertical-align: baseline
      if has-support('ie < 8')
        *vertical-align: middle

  // Address `overflow` set to `hidden` in IE 8/9/10/11.
  if has-support('ie')
    button
      overflow: visible

  // Address inconsistent `text-transform` inheritance for `button` and `select`.
  // All other form control elements do not inherit `text-transform` values.
  // Correct `button` style inheritance in Firefox, IE 8/9/10/11, and Opera
  // Correct `select` style inheritance in Firefox.
  button,
  select
    text-transform: none

  // 1. Avoid the WebKit bug in Android 4.0.* where (2) destroys native `audio`
  //     and `video` controls.
  // 2. Correct inability to style clickable `input` types in iOS.
  button,
  html input[type="button"], // 1
  input[type="reset"], // 2
  input[type="submit"]
    -webkit-appearance: button // 2

    // Improve usability and consistency of cursor style between image-type
    // `input` and others.
    cursor: pointer // 3

    // Remove inner spacing in IE 7 without affecting normal text inputs.
    // Known issue: inner spacing remains in IE 6.
    if has-support('ie < 8')
      *overflow: visible

  // Re-set default cursor for disabled elements.
  button[disabled],
  html input[disabled]
    cursor: default

  if has-support('firefox')
    // Remove inner padding and border in Firefox 3+.
    button::-moz-focus-inner,
    input::-moz-focus-inner
      border: 0
      padding: 0

    // Address Firefox 4+ setting `line-height` on `input` using `!important` in
    // the UA stylesheet.
    button,
    input
      line-height: initial('line-height')

  // It's recommended that you don't attempt to style these elements.
  // Firefox's implementation doesn't respect box-sizing, padding, or width.

  if has-support('ie < 11')
    input[type="checkbox"],
    input[type="radio"]
      // Address box sizing set to `content-box` in IE 8/9/10.
      box-sizing: border-box
      // Remove excess padding in IE 8/9/10.
      padding: 0
      if has-support('ie < 8')
        // Remove excess padding in IE 7.
        *height: 13px
        *width: 13px

  if has-support('chrome')
    // Fix the cursor style for Chrome's increment/decrement buttons. For
    // certain `font-size` values of the `input`, it causes the cursor style of
    // the decrement button to change from `default` to `text`.
    input[type="number"]::-webkit-inner-spin-button,
    input[type="number"]::-webkit-outer-spin-button
      height: auto

  if has-support('chrome', 'safari')
    input[type="search"]
      // Address `appearance` set to `searchfield` in Safari and Chrome.
      -webkit-appearance: textfield

      // Address `box-sizing` set to `border-box` in Safari and Chrome
      // (include `-moz` to future-proof)
      box-sizing: content-box

    // Remove inner padding and search cancel button in Safari and Chrome on
    // OS X Safari (but not Chrome) clips the cancel button when the search
    // input has padding (and `textfield` appearance).
    input[type="search"]::-webkit-search-cancel-button,
    input[type="search"]::-webkit-search-decoration
      -webkit-appearance: none

  // Define consistent border, margin, and padding.
  fieldset
    border: 1px solid #c0c0c0
    margin: 0 2px
    padding: 0.35em 0.625em 0.75em

  legend
    // Correct `color` not being inherited in IE 6/7/8/9/10/11
    if has-support('ie')
      border: 0

      // Correct alignment displayed oddly in IE 6/7
      if has-support('ie < 8')
        *margin-left: -7px

    // Correct text not wrapping in Firefox 3.
    if has-support('firefox < 4')
      white-space: normal

    // Remove padding so people aren't caught out if they zero out fieldsets.
    padding: 0

  textarea
    // Remove default vertical scrollbar in IE 8/9/10/11.
    if has-support('ie')
      overflow: auto

    vertical-align: top

  // Don't inherit the `font-weight` (applied by a rule above).
  // NOTE: the default cannot safely be changed in Chrome and Safari on OS X.
  optgroup
    font-weight: bold

  // Remove most spacing between table cells.
  table
    border-collapse: collapse
    border-spacing: 0

  td,
  th
    padding: 0